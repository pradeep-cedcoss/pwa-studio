        "graphql": "^14.2.1",
        "graphql-cli-validate-magento-pwa-queries": "file:///Users/jzetlen/gits/pwa/pwa-studio/packages/graphql-cli-validate-magento-pwa-queries/graphql-cli-validate-magento-pwa-queries-1.1.0-beta.1.tgz",
        "graphql-tag": "~2.10.1",
        "prop-types": "~15.7.2",
        "react": "~16.8.3",
        "react-apollo": "~2.5.5",
        "react-dom": "~16.8.3",
