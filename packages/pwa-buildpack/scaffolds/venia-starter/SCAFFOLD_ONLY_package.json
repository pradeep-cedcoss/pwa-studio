{
    "name": "based-on-venia",
    "private": true,
    "version": "0.0.1",
    "description": "A starter PWA Studio project based on Venia",
    "scripts": {
        "build": "npm run -s clean && buildpack validate-env . && npm run -s validate-queries && npm run -s build:prod",
        "build:analyze": "rimraf dist && mkdir dist && webpack -p --profile --no-progress --env.mode production --json > dist/build-stats.json && webpack-bundle-analyzer dist/build-stats.json",
        "build:prod": "webpack -p --color --progress --profile --env.mode production",
        "clean": "rimraf dist esm",
        "download-schema": "graphql get-schema --project venia",
        "start": "node server.js",
        "start:debug": "node --inspect-brk ./node_modules/.bin/webpack-dev-server --progress --color --env.mode development",
        "validate-queries": "npm run -s download-schema && graphql validate-magento-pwa-queries --project venia",
        "watch": "webpack-dev-server --progress --color --env.mode development"
    },
    "author": "@magento/create-pwa",
    "dependencies": {
        "@babel/runtime": "^7.4.2",
        "@magento/peregrine": "file:///Users/jzetlen/gits/pwa/pwa-studio/packages/peregrine/magento-peregrine-2.1.0.tgz",
